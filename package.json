{
  "name": "checkout_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "bin":{
    "checkout_api": "./dist/server.js"
  },
  "pkg":{
    "scripts": "server.js",
    "assets": [
      "dist/**",
      "node_modules/**",
      "prisma/**"
    ],
    "targets": ["node16-macos-x64"],
    "outputPath": "./"
  },
  "scripts": {
    "graph": "npx copyfiles -f src/schema/*.graphql dist/schema/",
    "compile": "tsc && npm run graph",
    "migrate:dev": "npx prisma migrate dev --name init",
    "seed:dev": "ts-node prisma/seeders/seeders.ts",
    "start:dev": "nodemon src/server.ts",
    "start:prod": "npm run compile && node dist/server.js",
    "test": "jest",
    "build": "npx pkg dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/schema": "^9.0.17",
    "@prisma/client": "^4.12.0",
    "cors": "^2.8.5",
    "graphql": "^16.6.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-tools/mock": "^8.7.19",
    "@jest/globals": "^29.5.0",
    "@types/cors": "^2.8.13",
    "@types/node": "^18.15.11",
    "graphql-import-node": "^0.0.5",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.22",
    "prisma": "^4.12.0",
    "prisma-mock": "^0.5.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
